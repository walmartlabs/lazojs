{
    "directories": {
        "application": "application",
        "components": "components",
        "models": "models"
    },
    "server": {
        "instances": {
            "primary": {
                "port": 8080
            }
        },
        "defaults": {
            "timeout": {
                "server": 30000,
                "client": 30000
            },
            "maxBytes": 10485760,
            "debug": false,
            "state": {
                "cookies": {
                    "failAction": "log",
                    "strictHeader": false
                }
            }
        },
        "maxSockets": 500
    },
    "client": {},
    "requirejs": {
        "common": {},
        "server": {},
        "client": {
            "shim": {
                "handlebars": {
                    "exports": "Handlebars"
                },
                "htmlparser": {
                    "exports": "Tautologistics.NodeHtmlParser"
                }
            },
            "paths": {}
        }
    },
    "libPath": "/lib/optimized/lib.js"
}